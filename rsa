#!/usr/bin/env python3

from sys import argv
from math import sqrt

def factorize(n):
	def is_prime(num):
		if num < 2:
			return False
		for i in range(2, int(sqrt(num)) + 1):
			if num % i == 0:
				return False
		return True

	factors = []

	while n % 2 == 0:
		factors.append(2)
		n //= 2

	for i in range(3, int(sqrt(n)) + 1, 2):
		while n % i == 0:
			factors.append(i)
			n //= i

	if n > 2:
		factors.append(n)

	return factors if len(factors) == 2 and is_prime(factors[0]) and is_prime(factors[1]) else None

if __name__ == "__main__":
	if len(argv) == 2:
		file_name = argv[1]
		with open(file_name, "r", encoding="utf-8") as file:
			lines = file.readlines()

		for line in lines:
			num = int(line.strip())
			factors = factorize(num)

			if factors:
				print("{}={}*{}".format(num, factors[1], factors[0]))
			else:
				print("{} cannot be factorized into two primes".format(num))

